<% include ./partials/header %>


<h1>Posts</h1>
<% posts.forEach((post)=>{ %>
<p id="<%=post._id %>">
    <h2><%= post.title %></h2>
    <h4><%=post.body %></h4>
    <a href="/post/<%=post._id %>/edit">Edit</a>
    <form action="/post/<%= post._id %>?_method=DELETE" method="post">
    <input type="submit" value="delete"></form>
    <hr />
</p>

<% }) %>
<% include ./partials/footer %>